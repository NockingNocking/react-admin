{
  "name": "react-study",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "yarn lint:js && yarn lint:stylelint && yarn lint:prettier",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:stylelint": "stylelint  src/**/*.{scss,css,sass,less} --fix --custom-syntax postcss-less",
    "preview": "vite preview",
    "eslint": "eslint",
    "pre-check": "tsc && npx lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn eslint"
    ],
    "src/**/*.{css,less,scss}": [
      "yarn stylelint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "ahooks": "^3.7.5",
    "axios": "^1.3.4",
    "commitizen": "^4.3.0",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "antd": "^5.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "mockjs": "^1.1.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-mock": "^2.9.6"
  },
  "main": "index.js",
  "repository": "git@github.com:NockingNocking/react-admin.git",
  "author": "xcw <497773732@qq.com>",
  "license": "MIT"
}
